<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Test</display-name>

	<welcome-file-list>

		<welcome-file>ajoutClient.jsp</welcome-file>

	</welcome-file-list>
	
	<servlet>

		<servlet-name>AjoutClientServlet</servlet-name>
		<servlet-class>com.intiformation.Servlets.AjoutClientServlet</servlet-class>

	</servlet>
	
	<servlet>

		<servlet-name>pageClientServlet</servlet-name>
		<servlet-class>com.intiformation.Servlets.pageClientServlet</servlet-class>

	</servlet>
	
	<servlet>

		<servlet-name>AccueilServlet</servlet-name>
		<servlet-class>com.intiformation.Servlets.AccueilServlet</servlet-class>

	</servlet>

	<servlet-mapping>

		<servlet-name>AjoutClientServlet</servlet-name>
		<url-pattern>/AjoutClientServlet</url-pattern>

	</servlet-mapping>
	<servlet-mapping>

		<servlet-name>pageClientServlet</servlet-name>
		<url-pattern>/pageClientServlet</url-pattern>

	</servlet-mapping>
	
	<servlet-mapping>

		<servlet-name>AccueilServlet</servlet-name>
		<url-pattern>/AccueilServlet</url-pattern>

	</servlet-mapping>

</web-app>